#!/bin/sh

echo "cabal-ghci doesn't work with ghc 7.10.3"
exit 1

#!/opt/ceh/lib/perl
# -*- mode: perl; -*-

use strict;
use warnings;
use lib "/opt/ceh/lib";
use Packages::GHC;
use CehBase;
use CehInstall;

foreach my $l (@ceh_ghc_libs) { path_prepend($l, 'LD_LIBRARY_PATH'); }

ceh_nixpkgs_install('haskellPackages.cabal-ghci', AUTOINIT);

exec "$ceh_nix_install_root/MAIN/bin/cabal-ghci", @ARGV;
