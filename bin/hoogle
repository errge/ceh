#!/opt/ceh/lib/perl
# -*- mode: perl; -*-

use strict;
use warnings;
use lib "/opt/ceh/lib";
use CehInstall;

ceh_nixpkgs_install('haskellPackages.hoogle', nixpkgs_version => '551296a1cec0b9751ab96c420a7481e322ea127d', out => '7cnxg6p68jng10xkncf1mpzyhpia0ack-hoogle-4.2.43');

chdir "$ENV{HOME}/.hoogle";
exec "$ceh_nix_install_root/MAIN/bin/hoogle", @ARGV;
