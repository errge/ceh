#!/bin/bash

echo '--------------------------------------------------------------------------------'
ulimit -av
free
sudo cat /proc/user_beancounters
echo '--------------------------------------------------------------------------------'

cd /opt/ceh
if [ "$TRAVIS_BRANCH" = "travis-baseline" ]; then
    echo '-------------------------------- FINAL PATCH -----------------------------------'
    git diff | cat
    echo '--------------------------------------------------------------------------------'
fi

echo $TRAVIS_TEST_RESULT

echo LAST FAILING SCRIPT OUTPUT:
ls -lart /tmp/installworld*
tail -n 100 `ls -1rt /tmp/installworld* | tail -n1`
