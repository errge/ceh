#!/bin/bash

echo '--------------------------------------------------------------------------------'
ulimit -av
free
sudo cat /proc/user_beancounters
echo '--------------------------------------------------------------------------------'

cd /opt/ceh
if [ "$TRAVIS_BRANCH" = "travis-baseline" ]; then
    echo '-------------------------------- FINAL PATCH -----------------------------------'
    git diff | cat
    echo '--------------------------------------------------------------------------------'
fi

if [ "$TRAVIS_TEST_RESULT" != 0 ]; then
    echo LAST FAILING SCRIPT OUTPUT:
    cd /tmp/installworld*
    ls -lart
    tail -n 100 `ls -1rt | tail -n1`

    echo DF AFTER THE FAILURE:
    df

    echo FREE AFTER THE FAILURE:
    free
fi
